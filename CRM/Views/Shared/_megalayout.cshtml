<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- CSS dependencies -->
    <link rel="stylesheet" type="text/css" href="~/Content/Theme/mega/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Theme/mega/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Theme/mega/css/main.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Theme/mega/css/font-style.css" />
    <script src="~/Content/Theme/mega/js/jquery-1.11.2.js"></script>
    <title>Express Video - @ViewBag.Title</title>
</head>
<body>

    <div class="pix_section pix_nav_menu pix_scroll_header normal gray-dark-bg" data-scroll-bg="#444">
        <div class="container">
            <div class="row">
                <div class="col-md-10 col-xs-12 pix-inner-col col-sm-10 column ui-droppable">
                    <div class="pix-content">
                        <nav class="navbar navbar-default pix-no-margin-bottom pix-navbar-default">
                            <div class="container-fluid">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#pix-navbar-collapse" aria-expanded="false">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                    <a class="navbar-brand logo-img logo-img-a pix-adjust-height" href="#"><img src="images/main/logo-thin-light.png" alt="" class="img-responsive pix-logo-img"></a>
                                </div>
                                <div class="collapse navbar-collapse" id="pix-navbar-collapse">
                                    <ul class="nav navbar-nav navbar-right media-middle pix-header-nav pix-adjust-height" id="pix-header-nav">
                                        <li class="dropdown">
                                            <a href="~/Home/Index" class="pix-gray">Home</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="~/Home/SampleVideo" class="pix-gray">Sample Video</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="pix-gray">Contact Us</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="pix-gray">About Us</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="pix-gray">Login</a>
                                        </li>
                                        @*<li class="dropdown">
                                            <a href="#" class="dropdown-toggle pix-gray" data-toggle="dropdown">
                                                DROPDOWN
                                            </a>
                                            <ul class="dropdown-menu dropdown-menu-left">
                                                <li>
                                                    <a href="#" class="null" data-toggle="null">Menu Builder</a>
                                                </li>
                                                <li>
                                                    <a href="#" class="null" data-toggle="null">Custom Link</a>
                                                </li>
                                                <li>
                                                    <a href="#" class="null" data-toggle="null">Integrations</a>
                                                </li>
                                            </ul>
                                        </li>*@
                                       
                                    </ul>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>
                <div class="col-md-2 col-xs-12 pix-inner-col col-sm-2">
                    <div class="pix-content pix-adjust-height text-right">
                        <div class="pix-header-item2 pix-padding-v-10">
                            <a href="#" class="pix-link-icon">
                                <i class="pixicon-facebook3 small-icon-25 pix-slight-white"></i>
                            </a>
                            <a href="#" class="pix-link-icon pix-padding-h-10">
                                <i class="pixicon-twitter4 small-icon-25 pix-slight-white"></i>
                            </a>
                            <a href="#" class="pix-link-icon">
                                <i class="pixicon-instagram4 small-icon-25 pix-slight-white"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    
    <div class="pix_section pix-padding-v-10 pix-sport-1">
        @RenderBody()
    </div>
    <!-- Javascript -->
    
    <script src="~/Content/Theme/mega/js/jquery-ui.js"></script>
    <script src="~/Content/Theme/mega/js/bootstrap.js"></script>
    <script src="~/Content/Theme/mega/js/velocity.min.js"></script>
    <script src="~/Content/Theme/mega/js/velocity.ui.min.js"></script>
    <script src="~/Content/Theme/mega/js/scroll.js" type="text/javascript"></script>
    <script src="~/Content/Theme/mega/js/custom.js"></script>
</body>
</html>
